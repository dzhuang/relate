[run]
source = .
parallel = True
branch=True
cover_pylib=False
omit =
    */.env/*
    */virtualenv*/*
    */setuptools*/*
    */migrations/*
    */mdx_mathjax.py
    contrib/*
    exercise-docker.py
    update-attempt-ids.py
    setup.py
    local_settings_example.py
    */wsgi.py
    */tests/*
    */tests.py

[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    def __repr__
    if self.debug
    if settings.Debug
    if debug
    debug_print
    if show_log

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError

    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:

    # mypy import
    if False:

ignore_errors = True
